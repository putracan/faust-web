{
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/",
      "destination": "/id/index.html",
      "permanent": false
    },
    {
      "source": "/index",
      "destination": "/id/index.html",
      "permanent": false
    },
    {
      "source": "/index.html",
      "destination": "/id/index.html",
      "permanent": false
    },
    {
      "source": "/en",
      "destination": "/en/index.html",
      "permanent": false
    },
    {
      "source": "/en/",
      "destination": "/en/index.html",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/id",
      "destination": "/id/index.html"
    },
    {
      "source": "/en",
      "destination": "/en/index.html"
    },
    {
      "source": "/en/:path*",
      "destination": "/en/:path*"
    },
    {
      "source": "/id/:path*",
      "destination": "/id/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    },
    {
      "source": "/src/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public,max-age=31536000,immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(js|css)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public,max-age=31536000,immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public,max-age=0,must-revalidate"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public,max-age=3600"
        }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public,max-age=3600"
        }
      ]
    }
  ]
}
